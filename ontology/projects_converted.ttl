gpo:Project_34 a gpo:Project ;
    gpo:belongsToDepartment gpo:Department_هندسة_البرمجيات ;
    gpo:hasAbstract "تهدف هذه الدراسة إلى تقييم دقة واعتمادية استخدام نموذج \"Constructive Cost Model - COCOMO\" كأداة لتقدير جهد إنجاز المشاريع البرمجية، بما في ذلك تحديد الوقت والتكلفة وعدد الأشخاص اللازمين لإتمام المشروع. يعتمد هذا النموذج في تقديراته على عدة عوامل، منها عدد أسطر الشفرة البرمجية، ومجموعة من العوامل المؤثرة على المشروع مثل تعقيد المنتج البرمجي ومستوى خبرة فريق تطوير المشروع. طُبّقت منهجية البحث العلمي على دراسة حالة مشروع برمجي معين، حيث قام الباحث بجمع خصائصه ومواصفاته وتحليلها وتحديد مقاييس لها، ومن ثم حساب تقديرات هذا المشروع باستخدام تطبيق ويب طوّره استنادا إلى معادلات حساب الجهد التي يستخدمها النموذج المذكور. قورنت نتائج حسابات النموذج مع آراء مجموعة من الخبراء في مجال تطوير وبيع المنتجات البرمجية، وأظهرت حسابات النموذج تقديرات مقاربة لآراء الخبراء. توصل الباحث في هذه الدراسة إلى أن استخدام \"COCOMO\" يمكن أن يكون مفيداً كأداة لتقدير جهد المشاريع البرمجية، ولكن يجب مراعاة العوامل المؤثرة على بيئة المشروع البرمجي لضمان دقة التقديرات."^^xsd:string ;
    gpo:hasAcademicYear "2023"^^xsd:gYear ;
    gpo:hasEnrolledStudent gpo:Student_34_1 ;
    gpo:hasProjectName "تقييم موثوقية COCOMO في تقدير جهد انجاز المشاريع البرمجية الصغيرة"^^xsd:string ;
    gpo:isSupervisedBySupervisor gpo:Supervisor_34 .

gpo:Project_35 a gpo:Project ;
